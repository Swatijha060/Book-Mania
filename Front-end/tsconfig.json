{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "composite": true,
    "outDir": "./dist/out-tsc",
    "rootDir": "./src",
    "declaration": true,
    "sourceMap": true,
    "removeComments": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    }
  }
}
